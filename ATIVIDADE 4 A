package AVFormativa;

import java.util.Scanner;

public class Ativrapeticao4a {

	public static void main(String[] args) {
	Scanner ler = new Scanner(System.in);
	
	int numero, soma = 0;
	
	for(int i=1; i<=10; i++) {
		System.out.print("Informe 10 números: ");
		numero = ler.nextInt();
		soma = soma+numero;
		System.out.println("O resultado da soma dos números é: " + soma);
		}
	}
}
